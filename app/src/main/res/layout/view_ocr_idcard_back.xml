<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout style="@style/item_attribute">

            <TextView
                style="@style/item_attribute_text"
                android:text="签发机关" />

            <TextView
                android:id="@+id/attribute_jiguan"
                style="@style/item_attribute_text2"
                tools:text="年龄年龄年龄年龄年龄年龄年龄年龄年" />
        </LinearLayout>

        <include layout="@layout/view_line" />

        <LinearLayout style="@style/item_attribute">

            <TextView
                style="@style/item_attribute_text"
                android:text="有效日期" />

            <TextView
                android:id="@+id/attribute_validate"
                style="@style/item_attribute_text2"
                tools:text="年龄年龄年龄年龄年龄年龄年龄年龄年" />
        </LinearLayout>

        <include layout="@layout/view_line" />

        <LinearLayout style="@style/item_attribute">

            <TextView
                style="@style/item_attribute_text"
                android:text="证件正反面" />

            <TextView
                android:id="@+id/attribute_front"
                style="@style/item_attribute_text2"
                tools:text="年龄年龄年龄年龄年龄年龄年龄年龄年" />
        </LinearLayout>

        <include layout="@layout/view_line" />

        <LinearLayout style="@style/item_attribute">

            <TextView
                style="@style/item_attribute_text"
                android:text="真实性" />

            <TextView
                android:id="@+id/attribute_real"
                style="@style/item_attribute_text2"
                tools:text="年龄年龄年龄年龄年龄年龄年龄年龄年" />
        </LinearLayout>
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>